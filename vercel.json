{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.0.13"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.js)$", "dest": "/$1" },
    { "src": "/(.*\\.css)$", "dest": "/$1" },
    { "src": "/(.*\\.html)$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
